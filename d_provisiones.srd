table(column=(type=char(20) updatewhereclause=yes name=ep_codigo dbname="no_cabrol.ep_codigo" )
 column=(type=char(20) updatewhereclause=yes name=cs_codigo dbname="no_emple.cs_codigo" )
 column=(type=datetime updatewhereclause=yes name=rr_fecha dbname="no_cabrol.rr_fecha" )
 column=(type=number updatewhereclause=yes name=rr_horas dbname="rr_horas" )
 column=(type=number updatewhereclause=yes name=rr_totingr dbname="rr_totingr" )
 column=(type=char(2) updatewhereclause=yes name=ru_codigo dbname="no_cabrol.ru_codigo" )
 column=(type=char(1) updatewhereclause=yes name=rr_pagado dbname="rr_pagado" )
 column=(type=number updatewhereclause=yes name=cc_dias_adic dbname="cc_dias_adic" )
 column=(type=datetime updatewhereclause=yes name=ep_fecafi dbname="no_emple.ep_fecafi" )
 retrieve="select x.ep_codigo,e.cs_codigo,x.rr_fecha ~"rr_fecha~",0 ~"rr_horas~",round(SUM(y.lq_total)*:ac_porcen/100,2) ~"rr_totingr~",:as_rucodigo ~"ru_codigo~",'S' ~"rr_pagado~",ceil((months_between(sysdate,e.ep_fecafi) - 60)/12) ~"cc_dias_adic~",e.ep_fecafi
from   no_cabrol  x,  no_rolpago y, no_emple e, no_rubro z
where  x. ep_codigo = y.ep_codigo
and    x.rr_codigo = y.rr_codigo
and    e.ep_codigo = x.ep_codigo
and    z.ru_codigo = y.ru_codigo
and 	 e.em_codigo = :empresa
and    to_char(x.rr_fecha,'MM/YYYY') = :mes
and    x.rr_tipo = 'R'
and    z.ru_sigla like 'I%' 
group by  x.ep_codigo,x.rr_fecha,e.ep_fecafi,e.cs_codigo

" arguments=(("empresa", string),("ac_porcen", number),("as_rucodigo", string),("mes", string))  sort="number(ep_codigo) A " )
