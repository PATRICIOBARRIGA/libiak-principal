 retrieve="SELECT 'PA',
         '3275916404',
         ROWNUM,
         ROWNUM,
         ~"NO_EMPLE~".~"EP_CI~",
        'USD',
         ~"NO_CABROL~".~"RR_LIQUIDO~"*100,
        'CTA',
        '10',
         DECODE(SUBSTR(EP_CTABCO,1,2),'AH','AHO','CC','CTE'),
         SUBSTR(EP_CTABCO,3),
         DECODE(LENGTH(EP_CI),10,'C',13,'R','N'),
         UPPER(~"NO_EMPLE~".~"EP_APEPAT~"||' '||~"NO_EMPLE~".~"EP_APEMAT~"||' '||~"NO_EMPLE~".~"EP_NOMBRE~"),
        UPPER (~"NO_EMPLE~".~"EP_CALLE1~"||' '||~"NO_EMPLE~".~"EP_CALLE2~"),
        ~"PR_CIUDAD~".~"CU_NOMBRE~",
        ~"NO_EMPLE~".~"EP_TELEF~",
        'QUINCENA'
FROM ~"NO_CABROL~",   
       ~"NO_EMPLE~" ,
        ~"PR_CIUDAD~"     
 WHERE ( ~"NO_EMPLE~".~"EP_CODIGO~" = ~"NO_CABROL~".~"EP_CODIGO~" ) and  
        (~"NO_EMPLE~".~"CU_CODIGO~" = ~"PR_CIUDAD~".~"CU_CODIGO~"(+)) and
       ( TO_CHAR(~"NO_CABROL~".~"RR_FECHA~",'MM/YYYY') = :as_fecha )  and
       ( TO_CHAR(~"NO_CABROL~".~"RR_FECHA~",'DD') <= '15' )  and
 	    ( ~"NO_EMPLE~".~"EP_NONOM~" = 'S') and
		 ( NVL(~"NO_EMPLE~".~"EP_CTABCO~",0) <> '0') and
		 ( ~"NO_CABROL~".~"RR_LIQUIDO~" <> 0) and
           (~"NO_CABROL~".~"RR_TIPO~" = 'Q')
ORDER BY ~"NO_EMPLE~".~"EP_APEPAT~"
" arguments=(("as_fecha", string)) )
