 retrieve="SELECT ~"CP_MOVIM~".~"TV_CODIGO~",   
         ~"CP_MOVIM~".~"TV_TIPO~",   
         ~"CP_MOVIM~".~"EM_CODIGO~",   
         ~"CP_MOVIM~".~"SU_CODIGO~",   
         ~"CP_MOVIM~".~"MP_CODIGO~",   
         ~"CP_MOVIM~".~"PV_CODIGO~",  
         (select pv_razons from in_prove where pv_codigo = ~"CP_MOVIM~".~"PV_CODIGO~") razons,
         ~"CP_MOVIM~".~"EC_CODIGO~",   
         ~"CP_MOVIM~".~"MP_FECHA~",   
         nvl(~"CP_MOVIM~".~"MP_VALOR~",0) mp_valor,   
         nvl(~"CP_MOVIM~".~"MP_VALRET~",0) mp_valret,
			nvl(~"CP_MOVIM~".~"MP_BASEIMP~",0) mp_baseimp,
			nvl(~"CP_MOVIM~".~"MP_RETEN~",0) mp_reten,
			nvl(~"CP_MOVIM~".~"MP_RETIVA~",0) mp_retiva,
         nvl(~"CP_MOVIM~".~"MP_SALDO~",0) mp_saldo,
         ~"CP_MOVIM~".~"CO_NUMERO~",    
         ~"CP_MOVIM~".~"CO_FACPRO~",   
        ~"CP_MOVIM~".~"MP_CONTAB~",   
         0,   
         '',   
         SYSDATE,   
         SYSDATE,   
         '',
         0 ~"ABONO~",
         ~"CP_MOVIM~".~"ESTADO~" ,
         ~"CP_MOVIM~".~"MP_FECPAGO~",
         ~"CP_MOVIM~".~"MP_NROREPOS~" 
FROM ~"CP_MOVIM~"
WHERE ( (~"CP_MOVIM~".~"TV_TIPO~" = 'C' ) AND  
			( ~"CP_MOVIM~".~"EM_CODIGO~" = :empresa ) AND  
      (~"CP_MOVIM~".~"SU_CODIGO~" = :sucursal ) AND 
       (~"CP_MOVIM~".~"MP_NROREPOS~" like :nrorepos) AND
		(~"CP_MOVIM~".~"MP_SALDO~" > 0) AND
      trunc(~"CP_MOVIM~".~"MP_FECHA~")  between :fec_inicial and :fec_final )
" arguments=(("empresa", string),("sucursal", string),("nrorepos", string),("fec_inicial", date),("fec_final", date))  sort="cp_movim_pv_codigo A cp_movim_mp_fecha A " )
