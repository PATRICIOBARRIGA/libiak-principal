 retrieve="   SELECT ~"PR_EMPRE~".~"EM_NOMBRE~",
~"PD_DETRTA~".~"IT_CODIGO1~",   
		~"PD_DETRTA~".~"RQ_CANTID~", 
  	    ~"PD_DETRTA~".~"RQ_CANTID2~", 
		D.~"IT_CODANT~" COD_INGREDIENTE,   
		D.~"IT_NOMBRE~" NOM_INGREDIENTE,
         C.~"IT_CODANT~" COD_ITEM,
        C.~"IT_NOMBRE~"  NOM_ITEM  
    FROM ~"IN_ITEM~" D,  ~"PD_DETRTA~",~"PD_RECETA~", ~"IN_ITEM~" C, ~"PR_EMPRE~" 
WHERE (~"PR_EMPRE~".~"EM_CODIGO~" = ~"PD_RECETA~".~"EM_CODIGO~")
AND (~"PD_RECETA~".~"IT_CODIGO~" = C.~"IT_CODIGO~")
AND (~"PD_RECETA~".~"RT_CODIGO~" = ~"PD_DETRTA~".~"RT_CODIGO~")
AND ( D.~"IT_CODIGO~" = ~"PD_DETRTA~".~"IT_CODIGO1~" )  
AND (~"PD_RECETA~".~"IT_CODIGO~" = :pepa)
AND (~"PD_RECETA~".~"ESTADO~"  = :tipo)   
" arguments=(("pepa", string),("tipo", string)) )
