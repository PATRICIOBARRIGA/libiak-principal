         ~"NO_DETPAG~".~"PP_NUMERO~",   
         ~"NO_DETPAG~".~"DP_NUMERO~",   
         ~"NO_DETPAG~".~"DP_VALCUO~",   
         ~"NO_DETPAG~".~"DP_FECPAG~",   
         ~"NO_DETPAG~".~"DP_CUOCANC~",   
         ~"NO_DETPAG~".~"ESTADO~"  
    FROM ~"NO_DETPAG~"  
   WHERE ( ~"NO_DETPAG~".~"EP_CODIGO~" = :empleado ) AND  
         ( ~"NO_DETPAG~".~"PP_NUMERO~" = :numero )    
