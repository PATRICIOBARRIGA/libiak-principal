 column=(type=char(50) updatewhereclause=yes name=nombre_sucursal dbname="nombre_sucursal" )
 column=(type=number updatewhereclause=yes name=valor dbname="valor" )
 column=(type=char(20) updatewhereclause=yes name=cp_tipmov_tv_codigo dbname="cp_tipmov.tv_codigo" )
 retrieve="

SELECT ~"CP_TIPMOV~".~"TV_TIPO~" TIPO, 
~"CP_MOVIM~".~"SU_CODIGO~",
~"CP_TIPMOV~".~"TV_DEXCRI~",
 (select su_nombre from pr_sucur where su_codigo=~"CP_MOVIM~".~"SU_CODIGO~" )  nombre_sucursal ,
 sum(nvl(~"CP_MOVIM~".~"MP_VALOR~" *-1, 0 )) valor ,   
         ~"CP_TIPMOV~".~"TV_CODIGO~" 
FROM ~"CP_MOVIM~",
~"CP_TIPMOV~"
WHERE ~"CP_MOVIM~".~"TV_CODIGO~" =  ~"CP_TIPMOV~".~"TV_CODIGO~"
 AND ~"CP_MOVIM~".~"EM_CODIGO~" = :empresa
AND ~"CP_MOVIM~".~"PV_CODIGO~" = :cod_proveedor
AND ~"CP_TIPMOV~".~"TV_TIPO~" = 'C'
GROUP BY  ~"CP_TIPMOV~".~"TV_TIPO~",
 ~"CP_MOVIM~".~"SU_CODIGO~",~"CP_TIPMOV~".~"TV_DEXCRI~",  ~"CP_TIPMOV~".~"TV_CODIGO~" 

UNION ALL

  SELECT  ~"CP_PAGO~".~"TV_TIPO~" TIPO,
~"CP_PAGO~".~"SU_CODIGO~", 
          ( select fp_descri from fa_formpag where fp_codigo = ~"CP_PAGO~".~"FP_CODIGO~") forma_pago,
          (select su_nombre from pr_sucur where su_codigo=~"CP_PAGO~".~"SU_CODIGO~" )  nombre_sucursal ,
         sum(nvl(~"CP_PAGO~".~"PM_VALOR~",0 )) ,   
         ~"CP_PAGO~".~"FP_CODIGO~"  
    FROM ~"CP_MOVIM~",   
         ~"CP_PAGO~"  
   WHERE ( ~"CP_PAGO~".~"TV_CODIGO~" = ~"CP_MOVIM~".~"TV_CODIGO~" ) and  
         ( ~"CP_PAGO~".~"TV_TIPO~" = ~"CP_MOVIM~".~"TV_TIPO~" ) and  
         ( ~"CP_PAGO~".~"EM_CODIGO~" = ~"CP_MOVIM~".~"EM_CODIGO~" ) and  
         ( ~"CP_PAGO~".~"SU_CODIGO~" = ~"CP_MOVIM~".~"SU_CODIGO~" ) and  
         ( ~"CP_PAGO~".~"MP_CODIGO~" = ~"CP_MOVIM~".~"MP_CODIGO~" ) and  
         ( ( ~"CP_PAGO~".~"TV_TIPO~" = 'D' ) AND  
         ( ~"CP_PAGO~".~"EM_CODIGO~" = :empresa ) AND  
         ( ~"CP_MOVIM~".~"PV_CODIGO~" = :cod_proveedor ) )   
group by   ~"CP_PAGO~".~"TV_TIPO~",~"CP_PAGO~".~"SU_CODIGO~",   ~"CP_PAGO~".~"FP_CODIGO~"
" arguments=(("empresa", string),("cod_proveedor", string)) )
