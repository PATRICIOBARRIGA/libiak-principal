 retrieve="  SELECT ~"CC_MOVIM~".~"MT_CODIGO~",   
         ~"CC_MOVIM~".~"TT_CODIGO~",   
         ~"CC_MOVIM~".~"TT_IOE~",   
         ~"CC_MOVIM~".~"VE_NUMERO~",   
         ~"CC_MOVIM~".~"CE_CODIGO~",   
         ~"CC_MOVIM~".~"ES_CODIGO~",   
         ~"CC_MOVIM~".~"EM_CODIGO~",   
         ~"CC_MOVIM~".~"MT_FECHA~",   
         ~"CC_MOVIM~".~"SU_CODIGO~",   
         ~"CC_MOVIM~".~"MT_VALOR~",   
         ~"CC_MOVIM~".~"MT_SALDO~",   
         ~"CC_MOVIM~".~"MT_VALRET~",   
         ~"CC_MOVIM~".~"MT_CTACOR~",   
         ~"CC_MOVIM~".~"RF_CODIGO~",   
         ~"CC_MOVIM~".~"BO_CODIGO~",   
         ~"CC_MOVIM~".~"IG_NUMERO~",   
         ~"CC_MOVIM~".~"MT_VALIVA~", 
         ~"FA_CLIEN~".~"CE_FIRMA~",
         ~"FA_CLIEN~".~"CE_RAZONS~"
    FROM ~"CC_MOVIM~",   
         ~"FA_CLIEN~"  
   WHERE ( ~"CC_MOVIM~".~"EM_CODIGO~" = ~"FA_CLIEN~".~"EM_CODIGO~" ) and  
         ( ~"CC_MOVIM~".~"CE_CODIGO~" = ~"FA_CLIEN~".~"CE_CODIGO~" ) and  
         ( ~"CC_MOVIM~".~"TT_IOE~" = 'C' ) AND  
         ( ~"CC_MOVIM~".~"EM_CODIGO~" = :empresa ) AND  
         (~"CC_MOVIM~".~"SU_CODIGO~" = :sucursal) AND  
         (~"CC_MOVIM~".~"MT_FECHA~"  between :ad_ini and :ad_fin  + 1 )
" update="CC_MOVIM" updatewhere=0 updatekeyinplace=no retrieve.asneeded=yes arguments=(("empresa", string),("sucursal", string),("ad_ini", date),("ad_fin", date))  sort="number(mt_codigo) A " )
