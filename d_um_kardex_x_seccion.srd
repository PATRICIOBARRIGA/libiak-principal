 retrieve="SELECT ~"IN_ITEBOD~".~"IB_STKINI~",   
         ~"IN_ITEBOD~".~"IB_STOCK~",   
		~"IN_MOVIM~".~"TM_CODIGO~",   
         ~"IN_MOVIM~".~"TM_IOE~",   
         ~"IN_MOVIM~".~"IT_CODIGO~",   
         ~"IN_MOVIM~".~"EM_CODIGO~",   
         ~"IN_MOVIM~".~"SU_CODIGO~",   
         ~"IN_MOVIM~".~"BO_CODIGO~",   
         ~"IN_MOVIM~".~"MV_NUMERO~",   
         ~"IN_MOVIM~".~"MV_CANTID~",   
         ~"IN_MOVIM~".~"MV_COSTO~",   
         ~"IN_MOVIM~".~"MV_COSTTRANS~",   
         ~"IN_MOVIM~".~"MV_COSTPROM~",
         ~"IN_MOVIM~".~"MV_FECHA~",   
         ~"IN_MOVIM~".~"MV_OBSERV~",   
         ~"IN_MOVIM~".~"VE_NUMERO~",   
         ~"IN_ITEM~".~"IT_CODANT~",   
         ~"IN_ITEM~".~"IT_NOMBRE~",   
         ~"IN_ITEM~".~"IT_COSTO~",   
         ~"IN_ITEM~".~"IT_PREFAB~",   
         ~"IN_ITEM~".~"IT_CODBAR~",   
         ~"IN_ITEM~".~"CL_CODIGO~",   
         ~"IN_ITEM~".~"IT_KIT~",   
         ~"IN_ITEM~".~"IT_VALSTK~"
    FROM ~"IN_MOVIM~",   
         ~"IN_ITEM~",   
         ~"IN_ITEBOD~"  
   WHERE ( ~"IN_MOVIM~".~"IT_CODIGO~" = ~"IN_ITEM~".~"IT_CODIGO~" ) and  
		( ~"IN_MOVIM~".~"EM_CODIGO~" = ~"IN_ITEM~".~"EM_CODIGO~" ) and  
         ( ~"IN_ITEBOD~".~"IT_CODIGO~" = ~"IN_ITEM~".~"IT_CODIGO~" ) and  
         ( ~"IN_ITEBOD~".~"EM_CODIGO~" = ~"IN_ITEM~".~"EM_CODIGO~" ) and  
		( ~"IN_ITEBOD~".~"EM_CODIGO~" = ~"IN_MOVIM~".~"EM_CODIGO~" ) and  
		( ~"IN_ITEBOD~".~"SU_CODIGO~" = ~"IN_MOVIM~".~"SU_CODIGO~" ) and  
		( ~"IN_ITEBOD~".~"BO_CODIGO~" = ~"IN_MOVIM~".~"BO_CODIGO~" ) and  
         ( ( ~"IN_MOVIM~".~"EM_CODIGO~" = 1 ) AND  
         ( ~"IN_MOVIM~".~"SU_CODIGO~" = :sucursal ) AND  
         ( ~"IN_MOVIM~".~"BO_CODIGO~" = :seccion ) AND  
         ( ~"IN_MOVIM~".~"IT_CODIGO~" = :codigo ) AND 
		(    ~"IN_MOVIM~".~"MV_FECHA~"  between :ad_fini and  :ad_fcorte + 1) )" arguments=(("sucursal", string),("seccion", string),("codigo", string),("ad_fini", date),("ad_fcorte", date),("ac_sktini", number),("as_valorado", string))  sort="cc_year A cc_mes A in_movim_mv_fecha A " )
