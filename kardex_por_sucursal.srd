 retrieve="SELECT ~"IN_MOVIM~".~"TM_CODIGO~",   
         ~"IN_MOVIM~".~"TM_IOE~",   
         ~"IN_MOVIM~".~"IT_CODIGO~",   
         ~"IN_MOVIM~".~"EM_CODIGO~",   
         ~"IN_MOVIM~".~"SU_CODIGO~",   
         ~"IN_MOVIM~".~"BO_CODIGO~",   
         ~"IN_MOVIM~".~"MV_NUMERO~",   
         ~"IN_MOVIM~".~"MV_CANTID~",   
         ~"IN_MOVIM~".~"MV_COSTO~",   
         ~"IN_MOVIM~".~"MV_FECHA~",   
         ~"IN_MOVIM~".~"MV_OBSERV~",   
         ~"IN_MOVIM~".~"VE_NUMERO~",   
         ~"IN_ITEM~".~"IT_CODANT~",   
         ~"IN_ITEM~".~"IT_NOMBRE~",   
         ~"IN_ITEM~".~"IT_COSTO~",   
         ~"IN_ITEM~".~"IT_PREFAB~",   
         ~"IN_ITEM~".~"IT_CODBAR~",   
         ~"IN_ITEM~".~"CL_CODIGO~",   
         ~"IN_ITEM~".~"IT_KIT~",   
         ~"IN_ITEM~".~"IT_VALSTK~",   
         ~"IN_ITESUCUR~".~"IT_STKINI~",   
         ~"IN_ITESUCUR~".~"IT_STOCK~",   
         ~"IN_ITESUCUR~".~"IT_STKDIS~"  
    FROM ~"IN_MOVIM~",   
         ~"IN_ITEM~",   
         ~"IN_ITESUCUR~"  
   WHERE ( ~"IN_MOVIM~".~"IT_CODIGO~" = ~"IN_ITEM~".~"IT_CODIGO~" ) and  
		( ~"IN_MOVIM~".~"EM_CODIGO~" = ~"IN_ITEM~".~"EM_CODIGO~" ) and  
         ( ~"IN_ITESUCUR~".~"IT_CODIGO~" = ~"IN_ITEM~".~"IT_CODIGO~" ) and  
         ( ~"IN_ITESUCUR~".~"EM_CODIGO~" = ~"IN_ITEM~".~"EM_CODIGO~" ) and  
		( ~"IN_ITESUCUR~".~"EM_CODIGO~" = ~"IN_MOVIM~".~"EM_CODIGO~" ) and  
		( ~"IN_ITESUCUR~".~"SU_CODIGO~" = ~"IN_MOVIM~".~"SU_CODIGO~" ) and  
         ( ( ~"IN_MOVIM~".~"EM_CODIGO~" = 1 ) AND  
         ( ~"IN_MOVIM~".~"SU_CODIGO~" = :sucursal ) AND  
         ( ~"IN_MOVIM~".~"IT_CODIGO~" = :codigo ) AND 
		(   ~"IN_MOVIM~".~"MV_FECHA~"  <= :ad_fcorte + 1) )

" arguments=(("sucursal", string),("codigo", string),("ad_fcorte", date),("as_valorado", string))  sort="in_movim_mv_fecha A " )
