~"PD_DETPED~",   
         ~"IN_ITEM~" ~"IN_ITEM_A~",   
         ~"IN_RELITEM~",   
         ~"IN_ITEM~" ~"IN_ITEM_B~"  
   WHERE (~"PD_PEDIDO~".~"PE_CODIGO~" = ~"PD_DETPED~".~"PE_CODIGO~") and
          ( ~"PD_DETPED~".~"IT_CODIGO~" = ~"IN_ITEM_A~".~"IT_CODIGO~" ) and  
         ( ~"IN_ITEM_A~".~"IT_CODIGO~" = ~"IN_RELITEM~".~"IT_CODIGO1~" ) and  
         ( ~"IN_RELITEM~".~"IT_CODIGO~" = ~"IN_ITEM_B~".~"IT_CODIGO~" ) 
group by  ~"IN_RELITEM~".~"IT_CODIGO~",   
