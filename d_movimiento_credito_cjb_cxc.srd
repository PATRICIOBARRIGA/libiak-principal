 retrieve="  SELECT ~"CC_MOVIM~".~"MT_CODIGO~",   
         ~"CC_MOVIM~".~"TT_CODIGO~",   
         ~"CC_MOVIM~".~"TT_IOE~",   
         ~"CC_MOVIM~".~"VE_NUMERO~",   
         ~"CC_MOVIM~".~"CE_CODIGO~",   
         ~"CC_MOVIM~".~"ES_CODIGO~",   
         ~"CC_MOVIM~".~"EM_CODIGO~",   
         ~"CC_MOVIM~".~"MT_FECHA~",   
         ~"CC_MOVIM~".~"SU_CODIGO~",   
         ~"CC_MOVIM~".~"MT_VALOR~",   
         ~"CC_MOVIM~".~"MT_SALDO~",   
         ~"CC_MOVIM~".~"MT_VALRET~",   
         ~"CC_MOVIM~".~"MT_CTACOR~",   
         ~"CC_MOVIM~".~"RF_CODIGO~",   
         ~"CC_MOVIM~".~"BO_CODIGO~",   
         ~"CC_MOVIM~".~"IG_NUMERO~",   
         ~"CC_MOVIM~".~"MT_VALIVA~", 
         ~"FA_CLIEN~".~"CE_FIRMA~"  
    FROM ~"CC_MOVIM~",   
         ~"FA_CLIEN~"  
   WHERE ( ~"CC_MOVIM~".~"EM_CODIGO~" = ~"FA_CLIEN~".~"EM_CODIGO~" ) and  
         ( ~"CC_MOVIM~".~"CE_CODIGO~" = ~"FA_CLIEN~".~"CE_CODIGO~" ) and  
         ( ~"CC_MOVIM~".~"TT_IOE~" = 'C' ) AND  
         ( ~"CC_MOVIM~".~"EM_CODIGO~" = :empresa ) AND  
         (~"CC_MOVIM~".~"SU_CODIGO~" = :sucursal) AND  
         (~"CC_MOVIM~".~"MT_CODIGO~" = :movcre) AND
         (~"CC_MOVIM~".~"TT_CODIGO~" = :tipo) AND    
         (~"CC_MOVIM~".~"MT_SALDO~" > 0)
" update="CC_MOVIM" updatewhere=1 updatekeyinplace=yes retrieve.asneeded=yes arguments=(("empresa", string),("sucursal", string),("movcre", string),("tipo", string))  sort="number(mt_codigo) A " )
