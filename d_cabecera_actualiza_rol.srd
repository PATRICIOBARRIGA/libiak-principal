 retrieve="  SELECT ~"NO_CABROL~".~"EP_CODIGO~",   
         ~"NO_EMPLE~".~"EP_APEPAT~",   
         ~"NO_EMPLE~".~"EP_APEMAT~",   
         ~"NO_EMPLE~".~"EP_NOMBRE~",   
         ~"NO_CABROL~".~"RR_HORAS~",   
         ~"NO_CABROL~".~"RR_TOTINGR~",   
         ~"NO_CABROL~".~"RR_TOTEGRE~",   
         ~"NO_CABROL~".~"RR_LIQUIDO~",   
         ~"NO_CABROL~".~"RR_TIPO~",   
         ~"NO_CABROL~".~"RR_FECHA~",   
         ~"NO_CABROL~".~"RR_CODIGO~",   
         ~"NO_EMPLE~".~"EM_CODIGO~",   
         ~"NO_EMPLE~".~"SU_CODIGO~",   
         ~"NO_EMPLE~".~"DT_CODIGO~",   
         ~"NO_CABROL~".~"RR_PAGADO~",   
         ~"NO_EMPLE~".~"EP_CI~"  
    FROM ~"NO_CABROL~",   
         ~"NO_EMPLE~"  
   WHERE ( ~"NO_EMPLE~".~"EP_CODIGO~" = ~"NO_CABROL~".~"EP_CODIGO~" ) and  
         ( ( to_char(~"NO_CABROL~".~"RR_FECHA~",'mm-yyyy') = :periodo ) AND  
         ( ~"NO_CABROL~".~"RR_TIPO~" = 'R' ) AND  
         ( ~"NO_EMPLE~".~"EM_CODIGO~" = :em_codigo ) )
    
" update="NO_CABROL" updatewhere=1 updatekeyinplace=yes arguments=(("em_codigo", string),("periodo", string))  sort="ep_apepat A ep_nombre A " )
