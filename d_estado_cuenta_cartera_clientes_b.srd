 retrieve="SELECT ~"FA_CLIEN~".~"CE_RAZONS~"||'  '||~"FA_CLIEN~".~"CE_NOMBRE~"||'  '||~"FA_CLIEN~".~"CE_APELLI~" CLIENTE,
~"CC_MOVIM~".~"CE_CODIGO~",
        ~"CC_MOVIM~".~"TT_CODIGO~",   
       ~"CC_MOVIM~".~"VE_NUMERO~", 
	   ~"CC_MOVIM~".~"TT_IOE~",   
       ~"CC_MOVIM~".~"MT_CODIGO~",   
       ~"CC_MOVIM~".~"SU_CODIGO~",   
       ~"CC_MOVIM~".~"MT_VALOR~",   
       ~"CC_MOVIM~".~"MT_SALDO~",   
       ~"CC_MOVIM~".~"MT_FECHA~",
       ~"CC_TIPO~".~"TT_DESCRI~",
		 ~"CC_MOVIM~".~"MT_CTACLI~",
		 0 REF2,
       ~"CC_MOVIM~".~"MT_NUMCH~",
       ~"PR_INSTFIN~".~"IF_NOMBRE~",
       ~"CC_MOVIM~".~"MT_FECVEN~",
       ~"CC_MOVIM~".~"MT_CTACOR~",
      ~"PR_SUCUR~".~"SU_NOMBRE~"
FROM   ~"CC_MOVIM~",   
       ~"CC_TIPO~" ,
~"FA_CLIEN~",
~"PR_INSTFIN~",
~"PR_SUCUR~" 
WHERE ( ~"CC_TIPO~".~"TT_CODIGO~" = ~"CC_MOVIM~".~"TT_CODIGO~" ) and  
      ( ~"CC_TIPO~".~"TT_IOE~" = ~"CC_MOVIM~".~"TT_IOE~" ) and  
      ( ~"CC_MOVIM~".~"EM_CODIGO~" = ~"CC_TIPO~".~"EM_CODIGO~" ) and  
      ( ~"CC_MOVIM~".~"IF_CODIGO~"  =  ~"PR_INSTFIN~".~"IF_CODIGO~" (+) ) and  
      (~"CC_MOVIM~".~"CE_CODIGO~" = ~"FA_CLIEN~".~"CE_CODIGO~") and
      (~"CC_MOVIM~".~"SU_CODIGO~" = ~"PR_SUCUR~".~"SU_CODIGO~") and
      ( ( ~"CC_MOVIM~".~"EM_CODIGO~" = :empresa ) AND  
        ( ~"CC_MOVIM~".~"TT_IOE~" = 'D' ) AND
        ( trunc(~"CC_MOVIM~".~"MT_FECHA~") between :fec_ini and :fec_fin ))
UNION ALL
SELECT  ~"FA_CLIEN~".~"CE_RAZONS~"||'  '||~"FA_CLIEN~".~"CE_NOMBRE~"||'  '||~"FA_CLIEN~".~"CE_APELLI~" CLIENTE,
~"CC_MOVIM~".~"CE_CODIGO~",
~"CC_MOVIM~".~"TT_CODIGO~",   
       ~"CC_MOVIM~".~"VE_NUMERO~", 
		~"CC_MOVIM~".~"TT_IOE~",
       ~"CC_MOVIM~".~"MT_CODIGO~",
       ~"CC_MOVIM~".~"SU_CODIGO~",
       ~"CC_CHEQUE~".~"CH_VALOR~", 
       ~"CC_MOVIM~".~"MT_SALDO~",
       ~"CC_CHEQUE~".~"CH_FECHA~",
       ~"FA_FORMPAG~".~"FP_DESCORTA~"  ,
		 ~"CC_CHEQUE~".~"CH_CUENTA~",
		 ~"CC_CHEQUE~".~"CH_SECUE~" REF2,
       ~"CC_CHEQUE~".~"CH_NUMERO~",
       ~"PR_INSTFIN~".~"IF_NOMBRE~",
       ~"CC_CHEQUE~".~"CH_FECEFEC~",
       ' ' Motivo,
       ~"PR_SUCUR~".~"SU_NOMBRE~"
FROM   ~"CC_CHEQUE~",   
       ~"CC_MOVIM~",
       ~"FA_CLIEN~",
	   ~"FA_FORMPAG~" ,
~"PR_INSTFIN~" ,
~"PR_SUCUR~"
WHERE ( ~"CC_CHEQUE~".~"TT_CODIGO~" (+) = ~"CC_MOVIM~".~"TT_CODIGO~" ) and  
      ( ~"CC_CHEQUE~".~"TT_IOE~" (+) = ~"CC_MOVIM~".~"TT_IOE~" ) and  
      ( ~"CC_CHEQUE~".~"EM_CODIGO~" (+) = ~"CC_MOVIM~".~"EM_CODIGO~" ) and  
      ( ~"CC_CHEQUE~".~"SU_CODIGO~" (+) = ~"CC_MOVIM~".~"SU_CODIGO~" ) and  
      ( ~"CC_CHEQUE~".~"MT_CODIGO~" (+) = ~"CC_MOVIM~".~"MT_CODIGO~" ) and  
      ( ~"CC_CHEQUE~".~"FP_CODIGO~" = ~"FA_FORMPAG~".~"FP_CODIGO~" (+) ) and  
      ( ~"CC_CHEQUE~".~"IF_CODIGO~" = ~"PR_INSTFIN~".~"IF_CODIGO~" (+) ) and  
      (~"CC_MOVIM~".~"CE_CODIGO~" = ~"FA_CLIEN~".~"CE_CODIGO~") and
      (~"CC_MOVIM~".~"SU_CODIGO~" = ~"PR_SUCUR~".~"SU_CODIGO~") and
      ( ( ~"CC_MOVIM~".~"EM_CODIGO~" = :empresa) AND  
      ( ~"CC_MOVIM~".~"TT_IOE~" = 'C' ) AND
      ( trunc(~"CC_CHEQUE~".~"CH_FECEFEC~") between :fec_ini and :fec_fin))

