 retrieve=" SELECT ~"CP_CRUCE~".~"CX_VALCRE~",   
         ~"CP_CRUCE~".~"CX_VALDEB~",   
         ~"CP_PAGO~".~"PM_NAUT~",   
         ~"CP_PAGO~".~"FP_CODIGO~",   
         ~"CP_PAGO~".~"PM_EMISION~",   
         ~"CP_PAGO~".~"PM_PREIMP~",   
         ~"CP_PAGO~".~"PM_SECUENCIA~",   
         ~"CP_PAGO~".~"PM_FECHA~",   
         ~"CP_PAGO~".~"PM_BSEIMP~",   
         ~"CP_PAGO~".~"PM_PORCTJE~",   
         ~"CP_PAGO~".~"PM_VALOR~",   
         ~"CP_PAGO~".~"PM_CODPCT~",   
         ~"CP_PAGO~".~"PM_NROEST~",   
         ~"CP_PAGO~".~"PM_NROPTO~",   
         ~"CP_PAGO~".~"PM_NROSEC~" ,
         ~"CP_PAGO~".~"TV_CODIGO~",
        ~"CP_PAGO~".~"TV_TIPO~", 
       ~"CP_PAGO~".~"EM_CODIGO~",
      ~"CP_PAGO~".~"SU_CODIGO~",
        ~"CP_PAGO~".~"MP_CODIGO~"
 FROM ~"CP_CRUCE~",   
         ~"CP_PAGO~"  
 WHERE ( ~"CP_CRUCE~".~"TV_CODDEB~" = ~"CP_PAGO~".~"TV_CODIGO~" ) AND
         ( ~"CP_CRUCE~".~"TV_TIPODEB~" = ~"CP_PAGO~".~"TV_TIPO~" ) AND
         ( ~"CP_CRUCE~".~"EM_CODIGO~" = ~"CP_PAGO~".~"EM_CODIGO~" ) AND
         ( ~"CP_CRUCE~".~"SU_CODIGO~" = ~"CP_PAGO~".~"SU_CODIGO~" ) AND
         ( ~"CP_CRUCE~".~"MP_CODDEB~" = ~"CP_PAGO~".~"MP_CODIGO~" ) AND
         ( ( ~"CP_CRUCE~".~"EM_CODIGO~" = :empresa) AND  
         ( ~"CP_CRUCE~".~"SU_CODIGO~" = :sucursal )   AND  
         ( ~"CP_CRUCE~".~"MP_CODIGO~" =  :nrocredito ) AND
         ( ~"CP_PAGO~".~"FP_CODIGO~" = 6 ) ) 
" arguments=(("empresa", string),("sucursal", string),("nrocredito", string)) )
