 retrieve="  SELECT trunc(~"FA_VENTA~".~"VE_FECHA~") ~"fecha~",   
         SUM(~"FA_VENTA~".~"VE_SUBTOT~") ~"subtotal~",   
         SUM(~"FA_VENTA~".~"VE_DESCUE~") ~"descuento~",   
         SUM(~"FA_VENTA~".~"VE_NETO~") ~"neto~",
         SUM(~"FA_VENTA~".~"VE_IVA~") ~"iva~",   
         SUM(~"FA_VENTA~".~"VE_CARGO~") ~"cargo~",   
		SUM(~"FA_VENTA~".~"VE_VALPAG~") ~"total~" 
    FROM ~"FA_VENTA~"
   WHERE ( ( ~"FA_VENTA~".~"EM_CODIGO~" = :empresa ) AND  
         ( ~"FA_VENTA~".~"SU_CODIGO~" = :sucursal ) AND  
         ( ~"FA_VENTA~".~"ES_CODIGO~" in (:estado) ) AND  
         ( ~"FA_VENTA~".~"VE_FECHA~" between :fec_ini and :fec_fin ) )    
	GROUP BY trunc(~"FA_VENTA~".~"VE_FECHA~")" arguments=(("estado", stringlist),("empresa", string),("sucursal", string),("fec_ini", date),("fec_fin", date))  sort="fecha A " )
