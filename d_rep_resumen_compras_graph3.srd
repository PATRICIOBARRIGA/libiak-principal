table(column=(type=char(2) updatewhereclause=yes name=cc_fecha dbname="cc_fecha" )
 column=(type=number updatewhereclause=yes name=cc_total dbname="cc_total" )
 retrieve="SELECT TO_CHAR(~"IN_COMPRA~".~"CO_FECHA~",'MM') CC_FECHA,  
         sum(~"IN_COMPRA~".~"CO_TOTAL~") cc_total
    FROM ~"IN_COMPRA~",   
         ~"IN_PROVE~"  
   WHERE ( ~"IN_PROVE~".~"EM_CODIGO~" = ~"IN_COMPRA~".~"EM_CODIGO~" ) and  
         ( ~"IN_PROVE~".~"PV_CODIGO~" = ~"IN_COMPRA~".~"PV_CODIGO~" ) and  
         ( ( ~"IN_COMPRA~".~"EC_CODIGO~" = :estado ) AND  
         ( ~"IN_COMPRA~".~"EM_CODIGO~" = :empresa ) AND  
         ( trunc(~"IN_COMPRA~".~"CO_FECHA~",'MM') between :ad_ini and :ad_fin ))   
 GROUP BY to_char(~"IN_COMPRA~".~"CO_FECHA~",'MM')
" arguments=(("estado", string),("empresa", string),("ad_ini", date),("ad_fin", date))  sort="cc_fecha A " )
