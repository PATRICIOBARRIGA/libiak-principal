least(MAX(Y.CH_FECEFEC) - trunc(D.MT_FECVEN),15),' 9 - 15 ',
least(MAX(Y.CH_FECEFEC) - trunc(D.MT_FECVEN),30),' 16 - 30 ','m$$HEX1$$e100$$ENDHEX$$s 30 ') rango,
e.ep_codigo,
e.ep_apepat||' '||e.ep_nombre empleado,
a.fp_descri,
b.fp_descri
FROM  CC_MOVIM X, CC_CRUCE Z, CC_CHEQUE Y ,CC_MOVIM D,FA_VENTA V,NO_EMPLE E,FA_RECPAG P,FA_FORMPAG A,FA_FORMPAG B
